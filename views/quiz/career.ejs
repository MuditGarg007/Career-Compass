<% layout("/layouts/boilerplate") %>
<body>
    <div class="row">
        <div class="col-8 offset-2">
            <h2><%= info[info.length-1] %></h2>
            <br><br>
            <% for(let i=0; i<info.length-1; i++){ %>
            <div class="subheading">
                <h4><%= info[i].split(':')[0] %></h4>
                <p>
                    <%= info[i].split(':')[1] %>
                </p>
            </div>
            <% } %>
            <br><br><br>    
            <a href="/"><button class="btn">Back to Home</button></a>
        </div>
    </div>
</body>

